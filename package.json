    "babel-jest": "^23.6.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^2.7.0",
    "jest": "24.8.0",
    "prettier": "^1.19.1",
    "regenerator-runtime": "^0.12.1",
